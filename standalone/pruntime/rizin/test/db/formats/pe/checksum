NAME=PE: checksum 1
FILE=bins/pe/a.exe
ARGS=-A
CMDS=i~csum
EXPECT=<<EOF
cmp.csum 0x00005ee2
hdr.csum 0x00005ee2
EOF
RUN

NAME=PE: checksum 2
FILE=bins/pe/a2.exe
ARGS=-A
CMDS=i~csum
EXPECT=<<EOF
cmp.csum 0x000079a3
hdr.csum 0x000079a3
EOF
RUN

