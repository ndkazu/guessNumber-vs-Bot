NAME=PE: corkami aslr-ld.exe - open
FILE=bins/pe/aslr-ld.exe
CMDS=q!
EXPECT=<<EOF
EOF
RUN

NAME=PE: corkami aslr-ld.exe - entrypoint
FILE=bins/pe/aslr-ld.exe
CMDS=s
EXPECT=<<EOF
0x401000
EOF
RUN

NAME=PE: corkami aslr-ld.exe - pi 1
FILE=bins/pe/aslr-ld.exe
CMDS=pi 1
EXPECT=<<EOF
inc dword [0x401088]
EOF
RUN
